/* pages/vip-show/vip-show.wxss */

.bag {
  width: 100%;
  height: 100vh;
}
.show-top {
  width: 100%;
  height: 100%;
  background: rgba(22, 33, 33, 0.5);
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
}

.show-top>view {
  width: 25%;
  height: 90px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.show-top>view image {
  width: 33px;
  height: 33px;
  margin: 0 auto;
  z-index: 99999;
}

page {
  background: none;
  position: relative;
}
